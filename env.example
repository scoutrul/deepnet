# API Configuration (Конфигурация API)
VITE_OPENAI_API_KEY=your_openrouter_api_key_here

# API Base URL (Базовый URL API)
VITE_API_BASE_URL=https://openrouter.ai/api/v1

# Chat Model (Модель чата) - OpenRouter поддерживает множество моделей
VITE_CHAT_MODEL=gpt-4o

# Примечание: OpenRouter API ключи начинаются с 'sk-or-'
# Note: OpenRouter API keys start with 'sk-or-'

# Request Timeout (Таймаут запроса)
VITE_REQUEST_TIMEOUT_MS=5000

# App Settings (Настройки приложения)
VITE_APP_TITLE=DeepNet Encyclopedia

# HTTP Headers (HTTP заголовки)
VITE_HTTP_REFERRER=http://localhost:5173
VITE_HTTP_TITLE=DeepNet Encyclopedia
